datasource postgres {
  provider = "postgresql"
  url      = "postgresql://postgres:docker@localhost:5432/pg-comp-model?schema=public"
}

generator photon {
  provider = "photonjs"
}

model Course {
  id        String   @default(cuid()) @id
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  coreq     String
  crossListed String
  description String
  geCode    String
  labCredits  String
  lectureCredits  String
  number  String
  prefix  String
  preorco String
  prereq  String
  subject String
  termsOffered  String
  title String
  totalCredits  String
}

model Competency {
  id        String   @default(cuid()) @id
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
}
